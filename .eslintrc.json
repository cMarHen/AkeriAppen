{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "no-console": 0,
    "linebreak-style": "off",
    "react/prop-types": 0,
    "valid-jsdoc": "warn",
    "require-jsdoc": "warn",
    "comma-dangle": "off",
    "react-hooks/exhaustive-deps": 0
  },
  "globals": {
    "document": "readonly",
    "sessionStorage": "writable",
    "fetch": "writable",
    "window": "readonly"
  }
}